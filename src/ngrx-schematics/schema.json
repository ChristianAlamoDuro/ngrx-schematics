{
  "$schema": "http://json-schema.org/schema",
  "$id": "reduxSchematics",
  "title": "Redux Schematics",
  "type": "object",
  "description": "Generate entities for redux",
  "properties": {
    "entity": {
      "type": "string",
      "description": "The name of the entity (singular)",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "Whats your entity name (singular)"
    }
  },
  "required": ["entity"]
}
